@page
@model Proyecto.App.Frontend.Pages.IngresoModel
@{
}

<head>
    <link rel="stylesheet" href="\css\estilosIngreso.css">
</head>

<body>

    <div class="contenedor-ingreso contenedor">
        <div class="imagen-ingreso">
            <form class="ingreso">
                <div class="texto-formulario">
                    <h2> Ingreso de usuario</h2>
                </div>
                <div class="input">
                    <label for="documento">Documento</label>
                    <input placeholder="Ingresa tu documento" type="text" id="documento" name="documento">
                </div>
                <div class="input">
                    <label for="nombre">Nombre</label>
                    <input placeholder="Ingresa tus nombres" type="text" id="nombre" name="nombre">
                </div>
                <div class="input">
                    <label for="segundoapellido">Apellidos</label>
                    <input placeholder="Ingresa tus apellidos" type="text" id="segundoapellido" name="segundoapellido">
                </div>
                <div class="input">
                    <input type="submit" value="Ingresar">

                </div>
            </form>
        </div>


    </div>
</body>